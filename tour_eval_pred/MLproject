name: tour_eval_pred
conda_env: conda.yml

entry_points:
  main:
    parameters:
      mlflow_model:
        description: An MLflow serialized model
        type: string
      step_description:
        description: This step checks if the previous tour predictions were correct and predicts the result of the next tour matches
        type: string
    command: "python run.py --mlflow_model {mlflow_model} --step_description {step_description}"
